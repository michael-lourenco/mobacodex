e {{ e5 }} <span class=\"color99FF99\">(+{{ a2 }})</span> de daño mágico.","leveltip":{"label":["Daño de Sello de la Maldad Imitado","Daño de Distorsión Imitado","Daño de Cadenas Etéreas Imitado","Enfriamiento"],"effect":["{{ e3 }} -> {{ e3NL }}","{{ e5 }} -> {{ e5NL }}","{{ e3 }} -> {{ e3NL }}","{{ cooldown }} -> {{ cooldownnNL }}"]},"maxrank":3,"cooldown":[40,32,24],"cooldownBurn":"40/32/24","cost":[0,0,0],"costBurn":"0","effect":[null,[20,40,60],[30,37.5,45],[100,200,300],[100,200,300],[150,300,450]],"effectBurn":[null,"20/40/60","30/37.5/45","100/200/300","100/200/300","150/300/450"],"vars":[{"link":"spelldamage","coeff":0.6,"key":"a1"},{"link":"spelldamage","coeff":0.9,"key":"a2"}],"costType":"NoCost","range":"self","rangeBurn":"self","image":{"full":"LeblancMimic.png","sprite":"spell5.png","group":"spell","x":96,"y":144,"w":48,"h":48},"altimages":[{"full":"LeblancMimic0.png","sprite":"spell5.png","group":"spell","x":144,"y":144,"w":48,"h":48},{"full":"LeblancMimic1.png","sprite":"spell5.png","group":"spell","x":192,"y":144,"w":48,"h":48},{"full":"LeblancMimic2.png","sprite":"spell5.png","group":"spell","x":240,"y":144,"w":48,"h":48}],"resource":"Sin Costo"}],"passive":{"name":"Imagen Especular","description":"Si la Vida de LeBlanc baja del 40%, se vuelve invisible durante medio segundo. Cuando la invisibilidad termina, crea una Imagen Especular que no inflige daño y dura hasta 8 segundos. <br><br> Imagen Especular tiene 1 minuto de Enfriamiento.<br><br><span class=\"color99FF99\">Imagen Especular puede controlarse manteniendo presionada la tecla Alt y usando el botón derecho del ratón.</span>","image":{"full":"LeblancMirrorImage.png","sprite":"passive1.png","group":"passive","x":432,"y":48,"w":48,"h":48}},"recommended":[{"champion":"Leblanc","title":"Beginner","map":"SR","mode":"INTRO","type":"riot","customTag":"","extensionPage":false,"customPanel":null,"blocks":[{"type":"beginner_starter","recMath":false,"recSteps":false,"minSummonerLevel":5,"maxSummonerLeve