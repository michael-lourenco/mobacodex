 debuff-ul ''Iluminare''.","leveltip":{"label":["Daune","Timp de reactivare"],"effect":["{{ e1 }} -> {{ e1NL }}","{{ cooldown }} -> {{ cooldownnNL }}"]},"maxrank":3,"cooldown":[80,65,50],"cooldownBurn":"80/65/50","cost":[100,100,0],"costBurn":"100/100/0","effect":[],"effectBurn":[],"vars":[{"link":"spelldamage","coeff":0.75,"key":"a1"}],"costType":"Mana","maxammo":"0","range":[3340,3340,3340],"rangeBurn":"3340","image":{"full":"LuxMaliceCannon.png","sprite":"spell6.png","group":"spell","x":48,"y":96,"w":48,"h":48},"resource":"{{ cost }} mană"}],"passive":{"name":"Iluminare","description":"Vrăjile lui Lux care provoacă daune încarcă ţinta cu energie timp de 6 secunde. Următorul atac al lui Lux aprinde energia, provocându-i ţintei daune magice bonus (în funcţie de nivelul lui Lux).","image":{"full":"LuxIlluminatingFraulein.png","sprite":"passive1.png","group":"passive","x":240,"y":96,"w":48,"h":48}},"recommended":[{"champion":"Lux","title":"Beginner","type":"riot","map":"SR","mode":"INTRO","priority":false,"blocks":[{"type":"beginner_Starter","maxSummonerLevel":4,"items":[{"id":"1056","count":1}]},{"type":"beginner_Starter","minSummonerLevel":5,"items":[{"id":"1056","count":1},{"id":"2003","count":1}]},{"type":"beginner_Advanced","recMath":true,"items":[{"id":"1028","count":1},{"id":"1027","count":1},{"id":"3010","count":1}]},{"type":"beginner_MovementSpeed","recMath":true,"items":[{"id":"1001","count":1},{"id":"3020","count":1}]},{"type":"beginner_LegendaryItem","recMath":true,"items":[{"id":"3010","count":1},{"id":"1026","count":1},{"id":"3027","count":1}]},{"type":"beginner_MoreLegendaryItems","items":[{"id":"3151","count":1},{"id":"3174","count":1},{"id":"3116","count":1},{"id":"3089","count":1}]}]},{"champion":"Lux","title":"LuxASC","map":"CS","mode":"ASCENSION","type":"riot","customTag":"","extensionPage":false,"customPanel":null,"blocks":[{"type":"starting","recMath":false,"recSteps":false,"minSummonerLevel":-1,"maxSummonerLevel":-1,"showIfSummonerSpell":"","hi